{
    "extends": [
        "plugin:@wordpress/eslint-plugin/recommended"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "wp": "readonly"
    },
    "rules": {
        "no-redeclare": ["error", { "builtinGlobals": false }],
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    },
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    }
} 